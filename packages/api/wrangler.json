{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "cdn-api",
  "main": "src/index.ts",
  "compatibility_date": "2025-02-15",
  "compatibility_flags": ["nodejs_compat"],
  "workers_dev": false,

  "vars": {
    "BENCHMARK_URL": "http://localhost:8081"
  },
  "d1_databases": [
    {
      "binding": "STATS_DB",
      "database_name": "stats",
      "database_id": "01929292929292929292929292929292"
    }
  ],
  "durable_objects": {
    "bindings": [
      {
        "name": "BENCHMARK_DO",
        "class_name": "BenchmarkDurableObject"
      }
    ]
  }
}
